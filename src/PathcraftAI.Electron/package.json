{
  "name": "pathcraftai-electron",
  "version": "1.0.0",
  "description": "PathcraftAI Electron module for POE Trade with lazy loading",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win"
  },
  "keywords": [
    "pathcraftai",
    "poe",
    "trade",
    "electron"
  ],
  "author": "PathcraftAI",
  "license": "MIT",
  "dependencies": {
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.pathcraftai.electron",
    "productName": "PathcraftAI Trade",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    }
  }
}
